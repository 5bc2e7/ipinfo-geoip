{
  "input": [
    {
      "type": "text",
      "action": "add",
      "args": {
        "name": "my_custom_ips",
        "uri": "./my_ipinfo_data.txt"
      }
    }
  ],
  "output": [
    {
      "type": "v2rayGeoIPDat",
      "action": "output",
      "args": {
        "outputDir": "./output",
        "outputName": "geoip-custom.dat",
        "wantedList": ["my_custom_ips"]
      }
    }
  ]
}
